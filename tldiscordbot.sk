	krworldcid: 650665242745110529
	lucascid: 696689315811426314
	botonoff0: 694153082506051614
	log: 692940254478598205
	karinid: 624274923455512577
	hunnetcid: 705295453616472117
	
	# token = 카린봇 토큰
	# name = 카린봇 이름
	
	token: 'your token'
	name: KaRin Bot v2

On script load:
	send "{@c} &b카린봇 &e로드 &a성공&f!" to console
	set {krbad::624274923455512577} to "624274923455512577"
	set {krbad2::624274923455512577} to "624274923455512577"
	set {krbad::539590846014685185} to "539590846014685185"
	set {krbad::688222392186699779} to "688222392186699779"
	set {krbpremium::624274923455512577} to true
	set {krbpremium::451335803038203904} to true
	set {krbad::608956812636454915} to "608956812636454915"
	set {krbad::703121163379015712} to "703121163379015712"
	set {krbad::451335803038203904} to "451335803038203904"
	#set {server::702363259990442044} to "702363259990442044"
	
On skript load:
	login to "{@token}" with the name "{@name}" # 옵션의 name이라는 이름으로 token이라는 토큰에 로그인 함.
	set {krbad::624274923455512577} to "624274923455512577"
	set {krbad2::624274923455512577} to "624274923455512577"
	set {krbad::539590846014685185} to "539590846014685185"
	set {krbad::688222392186699779} to "688222392186699779"
	set {krbpremium::624274923455512577} to true
	set {krbpremium::451335803038203904} to true
	create embed:
